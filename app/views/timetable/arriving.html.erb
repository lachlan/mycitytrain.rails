<%- @title = "Arriving" -%>
<%- @html_class = 'list' -%>
<ul class="list">
	<%- @stations.each do |station| -%>
		<%- if station.id == @departing.id -%>
			<li class='disabled'><div><%= h station.name -%></div></li>
		<%- else -%>
			<li><%= link_to (h station.name), { :controller => 'timetable', :action => 'add_favourite', :departing => @departing, :arriving => station } %></li>
		<%- end -%>
	<%- end -%>
</ul>