<% @title = "Arriving" %>
<% @html_class = 'list' %>
<ul class="list">
	<% @stations.each do |station| %>
		<% if station.id == @departing.id %>
			<li class="disabled"><%= h station.name %></li>		
		<% else %>
			<li><%= link_to (h station.name), :action => 'upcoming', :departing => @departing.code, :arriving => station.code  %></li>
		<% end %>
	<% end %>
</ul>